<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <androidx.cardview.widget.CardView
            android:id="@+id/fb_card"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="4dp"
            app:cardElevation="8dp"
            android:padding="10dp"
            android:layout_margin="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardElevation="8dp"
                    android:padding="10dp"
                    android:background="@color/colorGrey2">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/fb_img"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:padding="10dp"
                        android:src="@drawable/veggies_logo" />



                </androidx.cardview.widget.CardView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:padding="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Facebook"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:id="@+id/fb_title"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:textColor="@color/black_color"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/long_string"
                        android:maxLines="3"
                        android:ellipsize="end"
                        android:id="@+id/fb_description"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:textColor="@color/colorGrey1"
                        />


                </LinearLayout>

            </LinearLayout>


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/in_card"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:padding="10dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@color/colorGrey2"
                    android:padding="10dp"
                    app:cardElevation="8dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/in_img"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:padding="10dp"
                        android:src="@drawable/veggies_logo" />


                </androidx.cardview.widget.CardView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:padding="10dp">

                    <TextView
                        android:id="@+id/in_title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:text="InstaGram"
                        android:textColor="@color/black_color"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/in_description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:ellipsize="end"
                        android:maxLines="3"
                        android:text="@string/long_string"
                        android:textColor="@color/colorGrey1"
                        android:textSize="15sp" />


                </LinearLayout>



            </LinearLayout>


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/tw_card"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="4dp"
            app:cardElevation="8dp"
            android:padding="10dp"
            android:layout_margin="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardElevation="8dp"
                    android:padding="10dp"
                    android:background="@color/colorGrey2">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/tw_img"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:padding="10dp"
                        android:src="@drawable/veggies_logo" />



                </androidx.cardview.widget.CardView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:padding="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Twitter"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:id="@+id/tw_title"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:textColor="@color/black_color"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/long_string"
                        android:maxLines="3"
                        android:ellipsize="end"
                        android:id="@+id/tw_description"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:textColor="@color/colorGrey1"
                        />


                </LinearLayout>

            </LinearLayout>


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/yt_card"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="4dp"
            app:cardElevation="8dp"
            android:padding="10dp"
            android:layout_margin="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardElevation="8dp"
                    android:padding="10dp"
                    android:background="@color/colorGrey2">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/yt_img"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:padding="10dp"
                        android:src="@drawable/veggies_logo" />



                </androidx.cardview.widget.CardView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:padding="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Youtube"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:id="@+id/yt_title"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:textColor="@color/black_color"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/long_string"
                        android:maxLines="3"
                        android:ellipsize="end"
                        android:id="@+id/yt_description"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:textColor="@color/colorGrey1"
                        />


                </LinearLayout>

            </LinearLayout>


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/lin_card"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="4dp"
            app:cardElevation="8dp"
            android:padding="10dp"
            android:layout_margin="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardElevation="8dp"
                    android:padding="10dp"
                    android:background="@color/colorGrey2">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/lin_img"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:padding="10dp"
                        android:src="@drawable/veggies_logo" />



                </androidx.cardview.widget.CardView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:padding="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Linked"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:id="@+id/lin_title"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:textColor="@color/black_color"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/long_string"
                        android:maxLines="3"
                        android:ellipsize="end"
                        android:id="@+id/lin_description"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:textColor="@color/colorGrey1"
                        />


                </LinearLayout>

            </LinearLayout>


        </androidx.cardview.widget.CardView>
    </LinearLayout>


</RelativeLayout>